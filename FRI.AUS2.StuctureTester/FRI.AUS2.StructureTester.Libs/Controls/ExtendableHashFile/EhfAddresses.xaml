<UserControl x:Class="FRI.AUS2.StructureTester.Libs.Controls.ExtendableHashFile.EhfAddresses"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:FRI.AUS2.StructureTester.Libs.Controls.ExtendableHashFile"
    xmlns:libsuc="clr-namespace:FRI.AUS2.StructureTester.Libs.Controls"
    mc:Ignorable="d"
    d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel Orientation="Vertical">
        <libsuc:InlineText x:Name="_txt_StatsDepth" Title="Depth" Value="1" />
        <libsuc:InlineText x:Name="_txt_StatsAddressesCount" Title="AddressesCount" Value="1" />

        <GroupBox Header="Addresses" MaxHeight="150">
            <ScrollViewer>
                <ListBox x:Name="_lstbx_Addresses" />
            </ScrollViewer>
        </GroupBox>
    </StackPanel>
</UserControl>